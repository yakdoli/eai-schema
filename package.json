{
  "name": "eai-schema-toolkit",
  "version": "1.0.0",
  "description": "EAI Schema Toolkit - Enterprise Application Integration Schema Conversion and Validation Tool",
  "homepage": "https://[username].github.io/eai-schema/",
  "repository": {
    "type": "git",
    "url": "https://github.com/[username]/eai-schema.git"
  },
  "bugs": {
    "url": "https://github.com/[username]/eai-schema/issues"
  },
  "keywords": [
    "eai",
    "schema",
    "xml",
    "json",
    "yaml",
    "conversion",
    "validation",
    "enterprise"
  ],
  "main": "dist/index.js",
  "scripts": {
    "build": "npx tsc",
    "dev": "tsx watch src/index.ts",
    "start": "node dist/index.js",
    "test": "jest",
    "lint": "eslint src --ext .ts",
    "lint:fix": "eslint src --ext .ts --fix",
    "clean": "rimraf dist",
    "deploy": "npm run build && gh-pages -d docs",
    "predeploy": "npm run build"
  },
  "dependencies": {
    "@eai/shared": "workspace:*",
    "@eai/schema-parser": "workspace:*",
    "@eai/schema-converter": "workspace:*",
    "@eai/schema-validator": "workspace:*",
    "@eai/sap-adapters": "workspace:*",
    "express": "^4.18.0",
    "cors": "^2.8.5",
    "helmet": "^7.0.0",
    "morgan": "^1.10.0",
    "multer": "^1.4.5-lts.1",
    "express-rate-limit": "^6.8.0",
    "compression": "^1.7.4",
    "winston": "^3.10.0",
    "dotenv": "^16.3.0"
  },
  "devDependencies": {
    "@types/express": "^4.17.0",
    "@types/cors": "^2.8.0",
    "@types/morgan": "^1.9.0",
    "@types/multer": "^1.4.0",
    "@types/compression": "^1.7.0",
    "@types/node": "^20.0.0",
    "@types/jest": "^29.5.0",
    "@types/supertest": "^2.0.12",
    "jest": "^29.5.0",
    "ts-jest": "^29.1.0",
    "supertest": "^6.3.0",
    "tsx": "^3.12.0",
    "typescript": "^5.0.0",
    "rimraf": "^5.0.0",
    "gh-pages": "^6.0.0"
  },
  "engines": {
    "node": ">=18.0.0",
    "npm": ">=8.0.0"
  }
}
