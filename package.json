{
  "name": "eai-schema-toolkit",
  "version": "1.0.0",
  "description": "EAI Schema Toolkit - Enterprise Application Integration Schema Conversion and Validation Tool",
  "homepage": "https://yakdoli.github.io/eai-schema/",
  "repository": {
    "type": "git",
    "url": "https://github.com/yakdoli/eai-schema.git"
  },
  "bugs": {
    "url": "https://github.com/yakdoli/eai-schema/issues"
  },
  "keywords": [
    "eai",
    "schema",
    "xml",
    "json",
    "yaml",
    "conversion",
    "validation",
    "enterprise"
  ],
  "main": "dist/index.js",
  "scripts": {
    "build": "npx tsc",
    "dev": "tsx watch src/index.ts",
    "start": "node dist/index.js",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "test:integration": "jest --testPathPattern=integration",
    "test:unit": "jest --testPathPattern=services",
    "test:e2e": "playwright test",
    "test:e2e:headed": "playwright test --headed",
    "test:performance": "jest --testPathPattern=performance --runInBand --detectOpenHandles",
    "test:load": "artillery run src/__tests__/performance/load-test.config.yml",
    "test:all-integration": "npm run test:integration && npm run test:e2e && npm run test:performance",
    "lint": "eslint src",
    "lint:fix": "eslint src --fix",
    "format": "prettier --write \"src/**/*.{ts,js,json}\"",
    "format:check": "prettier --check \"src/**/*.{ts,js,json}\"",
    "type-check": "tsc --noEmit",
    "clean": "rimraf dist",
    "prebuild": "npm run clean",
    "postbuild": "echo \"Build completed successfully\"",
    "deploy": "npm run build && gh-pages -d docs",
    "predeploy": "npm run build",
    "heroku-prebuild": "echo \"Prebuild script executed\"",
    "heroku-postbuild": "npm run build",
    "test:all": "./scripts/test-runner.sh",
    "security:audit": "npm audit --audit-level=moderate",
    "security:fix": "npm audit fix",
    "security:licenses": "license-checker --summary",
    "release": "semantic-release",
    "release:dry": "semantic-release --dry-run",
    "ci:validate": "npm run type-check && npm run lint && npm run format:check",
    "ci:test": "npm run test:unit && npm run test:integration",
    "ci:build": "npm run build",
    "mcp:start": "cd mcp-server && npm start",
    "mcp:install": "cd mcp-server && npm install"
  },
  "dependencies": {
    "@types/bcrypt": "^6.0.0",
    "@types/express-validator": "^2.20.33",
    "@types/handsontable": "^0.31.2",
    "@types/js-yaml": "^4.0.9",
    "@types/jsonwebtoken": "^9.0.10",
    "@types/swagger-jsdoc": "^6.0.4",
    "@types/swagger-ui-express": "^4.1.6",
    "@types/xml2js": "^0.4.14",
    "ajv": "^8.17.1",
    "ajv-formats": "^2.1.1",
    "bcrypt": "^6.0.0",
    "compression": "^1.7.4",
    "cors": "^2.8.5",
    "dotenv": "^16.4.5",
    "express": "^5.1.0",
    "express-rate-limit": "^7.4.0",
    "express-validator": "^7.2.1",
    "fast-xml-parser": "^4.4.1",
    "handsontable": "^16.0.1",
    "helmet": "^7.1.0",
    "js-yaml": "^4.1.0",
    "jsonwebtoken": "^9.0.2",
    "libxmljs2": "^0.37.0",
    "morgan": "^1.10.0",
    "multer": "^1.4.5-lts.1",
    "prom-client": "^15.1.3",
    "prometheus-gc-stats": "^1.1.0",
    "socket.io": "^4.8.1",
    "swagger-jsdoc": "^6.2.8",
    "swagger-ui-express": "^5.0.1",
    "winston": "^3.15.0",
    "xml-js": "^1.6.11",
    "xml2js": "^0.6.2",
    "xmldom": "^0.6.0",
    "yaml-validator": "^5.1.0",
    "zod": "^4.1.5"
  },
  "devDependencies": {
    "@eslint/js": "^9.15.0",
    "@playwright/test": "^1.55.0",
    "@semantic-release/changelog": "^6.0.3",
    "@semantic-release/commit-analyzer": "^13.0.0",
    "@semantic-release/git": "^10.0.1",
    "@semantic-release/github": "^11.0.0",
    "@semantic-release/npm": "^12.0.1",
    "@semantic-release/release-notes-generator": "^14.0.1",
    "@types/compression": "^1.7.4",
    "@types/cors": "^2.8.16",
    "@types/express": "^5.0.0",
    "@types/jest": "^29.5.14",
    "@types/morgan": "^1.9.7",
    "@types/multer": "^1.4.11",
    "@types/node": "^22.9.0",
    "@types/socket.io": "^3.0.1",
    "@types/supertest": "^6.0.1",
    "@typescript-eslint/eslint-plugin": "^8.15.0",
    "@typescript-eslint/parser": "^8.15.0",
    "artillery": "^2.0.24",
    "autocannon": "^8.0.0",
    "clinic": "^13.0.0",
    "conventional-changelog-conventionalcommits": "^8.0.0",
    "eslint": "^9.15.0",
    "gh-pages": "^6.1.1",
    "jest": "^29.7.0",
    "license-checker": "^25.0.1",
    "playwright": "^1.55.0",
    "prettier": "^3.4.2",
    "rimraf": "^5.0.10",
    "semantic-release": "^24.2.0",
    "socket.io-client": "^4.8.1",
    "supertest": "^6.3.4",
    "ts-jest": "^29.2.5",
    "tsx": "^4.19.1",
    "typescript": "^5.7.2"
  },
  "engines": {
    "node": ">=22.0.0",
    "npm": ">=10.0.0"
  }
}
